{
  "name": "wonderschool-gql",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "json-graphql-server db.js & webpack-serve --port 8000",
    "build": "webpack --env.production",
    "prebuild": "yarn test",
    "server": "json-graphql-server db.js",
    "lint": "tslint --project . ",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/addon-storyshots": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/luxon": "^0.5.2",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/recompose": "^0.26.1",
    "@types/storybook__addon-storyshots": "^3.4.1",
    "@types/storybook__react": "^3.0.7",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "4.0.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "json-graphql-server": "^2.1.1",
    "react-hot-loader": "^4.1.2",
    "storybook": "^1.0.0",
    "styled-components": "^3.2.6",
    "ts-jest": "^22.4.5",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.3",
    "webpack-serve": "^0.3.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.0.9",
    "apollo-link-http": "^1.5.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.1",
    "luxon": "^1.2.0",
    "react": "^16.3.2",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.27.0",
    "url-loader": "^1.0.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^app(.*)": "<rootDir>/src/$1"
    }
  }
}
