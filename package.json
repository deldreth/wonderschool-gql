{
  "name": "wonderschool-gql",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "json-graphql-server db.js & webpack-serve --port 8000",
    "build": "webpack --env.production",
    "server": "json-graphql-server db.js",
    "lint": "tslint --project . ",
    "gen-types": "apollo-codegen introspect-schema src/schema.graphql --output src/schema.json && apollo-codegen generate ./src/graph/**/*.ts --schema src/schema.json --target typescript --output src/graph/types.ts"
  },
  "devDependencies": {
    "@types/luxon": "^0.5.2",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/recompose": "^0.26.1",
    "@types/webpack-env": "^1.13.6",
    "babel-runtime": "^6.26.0",
    "html-webpack-plugin": "^3.2.0",
    "json-graphql-server": "^2.1.1",
    "react-hot-loader": "^4.1.2",
    "styled-components": "^3.2.6",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.3",
    "webpack-serve": "^0.3.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.0.9",
    "apollo-link-http": "^1.5.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.1",
    "luxon": "^1.2.0",
    "react": "^16.3.2",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.27.0",
    "url-loader": "^1.0.1"
  }
